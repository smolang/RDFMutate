@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix idlab-fn: <http://example.com/idlab/function/>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@prefix eu-country: <http://publications.europa.eu/resource/authority/country/>.
@prefix era: <http://data.europa.eu/949/>.
@prefix era-sn: <http://data.europa.eu/949/concepts/sol-natures/>.

:rules_000 a void:Dataset;
    void:exampleResource :map_sections-of-line_000.
:map_sections-of-line_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source :database_000;
    rml:query "SELECT (SectionOfLines.Length * 1000) AS Length, UICLines.UICCode, MemberStates.Code, (SELECT LTRIM(RTRIM(UOPID)) FROM RINF.dbo.OperationalPoints, RINF.dbo.LineNodes WHERE OperationalPointID=OperationalPoints.ID AND LineNodes.ID=LineEdges.LineNodeStartID AND OperationalPoints.VersionID=LineNodes.VersionID) AS FROM_NODE, (SELECT LTRIM(RTRIM(UOPID)) FROM RINF.dbo.OperationalPoints, RINF.dbo.LineNodes WHERE OperationalPointID=OperationalPoints.ID AND LineNodes.ID=LineEdges.LineNodeEndID AND OperationalPoints.VersionID=LineNodes.VersionID) AS TO_NODE, REPLACE(LookupValues.[Value], ' ', '_') AS SolNature, SectionOfLines.IMCode FROM RINF.dbo.LineEdges, RINF.dbo.SectionOfLines, RINF.dbo.UICLines, RINF.dbo.MemberStateVersions, RINF.dbo.MemberStates, RINF.dbo.LookupValues WHERE LineEdges.ID = SectionOfLines.LineEdgeID AND LineEdges.VersionID = SectionOfLines.VersionID AND SectionOfLines.UICLineID = UICLines.ID AND SectionOfLines.VersionID = UICLines.VersionID AND SectionOfLines.VersionID = MemberStateVersions.ID AND SectionOfLines.SOLNature = LookupValues.ID AND MemberStateVersions.MemberStateID = MemberStates.ID";
    rr:sqlVersion rr:SQL2008.
:database_000 a d2rq:Database;
    d2rq:jdbcDSN "${RINF_HOST}";
    d2rq:jdbcDriver "com.microsoft.sqlserver.jdbc.SQLServerDriver";
    d2rq:username "${RINF_USER}";
    d2rq:password "${RINF_PWD}".
:source_000 rml:referenceFormulation ql:CSV.
:map_sections-of-line_000 a rr:TriplesMap;
    rdfs:label "sections-of-line".
:s_000 a rr:SubjectMap.
:map_sections-of-line_000 rr:subjectMap :s_000.
:s_000 rr:template "http://data.europa.eu/949/functionalInfrastructure/sectionsOfLine/{UICCode}_{FROM_NODE}_{TO_NODE}";
    rr:graphMap :gm_000.
:gm_000 a rr:GraphMap;
    rr:constant "${NAMED_GRAPH}".
:pom_000 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant rdf:type.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant era:SectionOfLine;
    rr:termType rr:IRI.
:pom_001 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant rdfs:label.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rr:template "{UICCode}_{FROM_NODE}_{TO_NODE}";
    rr:termType rr:Literal.
:pom_002 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant era:imCode.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rml:reference "IMCode";
    rr:termType rr:Literal.
:pom_003 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_003.
:pm_003 a rr:PredicateMap.
:pom_003 rr:predicateMap :pm_003.
:pm_003 rr:constant era:lineNationalId.
:pom_003 rr:objectMap :om_003.
:om_003 a rr:ObjectMap;
    rr:template "http://data.europa.eu/949/functionalInfrastructure/nationalLines/{UICCode}";
    rr:termType rr:IRI.
:pom_004 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_004.
:pm_004 a rr:PredicateMap.
:pom_004 rr:predicateMap :pm_004.
:pm_004 rr:constant era:opStart.
:pom_004 rr:objectMap :om_004.
:om_004 a rr:ObjectMap;
    rr:template "http://data.europa.eu/949/functionalInfrastructure/operationalPoints/{FROM_NODE}";
    rr:termType rr:IRI.
:pom_005 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_005.
:pm_005 a rr:PredicateMap.
:pom_005 rr:predicateMap :pm_005.
:pm_005 rr:constant era:opEnd.
:pom_005 rr:objectMap :om_005.
:om_005 a rr:ObjectMap;
    rr:template "http://data.europa.eu/949/functionalInfrastructure/operationalPoints/{TO_NODE}";
    rr:termType rr:IRI.
:pom_006 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_006.
:pm_006 a rr:PredicateMap.
:pom_006 rr:predicateMap :pm_006.
:pm_006 rr:constant era:length.
:pom_006 rr:objectMap :om_006.
:om_006 a rr:ObjectMap;
    rml:reference "Length";
    rr:termType rr:Literal;
    rr:datatype xsd:double.
:pom_007 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_007.
:pm_007 a rr:PredicateMap.
:pom_007 rr:predicateMap :pm_007.
:pm_007 rr:constant era:solNature.
:pom_007 rr:objectMap :om_007.
:om_007 a rr:ObjectMap;
    rr:template "http://data.europa.eu/949/concepts/sol-natures/{SolNature}";
    rr:termType rr:IRI.
:pom_008 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_008.
:pm_008 a rr:PredicateMap.
:pom_008 rr:predicateMap :pm_008.
:pm_008 rr:constant era:hasAbstraction.
:pom_008 rr:objectMap :om_008.
:om_008 a rr:ObjectMap;
    rr:template "http://data.europa.eu/949/topology/netElements/{UICCode}_{FROM_NODE}_{TO_NODE}";
    rr:termType rr:IRI.
:pom_009 a rr:PredicateObjectMap.
:map_sections-of-line_000 rr:predicateObjectMap :pom_009.
:pm_009 a rr:PredicateMap.
:pom_009 rr:predicateMap :pm_009.
:pm_009 rr:constant era:inCountry.
:rules_000 void:exampleResource :map_eu-country_000.
:map_eu-country_000 rml:logicalSource :source_001.
:source_001 a rml:LogicalSource;
    rml:source "data/countries.csv";
    rml:referenceFormulation ql:CSV.
:map_eu-country_000 a rr:TriplesMap;
    rdfs:label "eu-country".
:s_001 a rr:SubjectMap.
:map_eu-country_000 rr:subjectMap :s_001.
:s_001 rml:reference "eu".
:pom_009 rr:objectMap :om_009.
:om_009 a rr:ObjectMap;
    rr:parentTriplesMap :map_eu-country_000;
    rr:joinCondition :jc_000.
:jc_000 rr:child "Code";
    rr:parent "code".
