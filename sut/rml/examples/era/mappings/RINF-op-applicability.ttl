@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix era: <http://data.europa.eu/949/>.

:rules_000 a void:Dataset;
    void:exampleResource :map_op-tracks-not-applicable_000.
:map_op-tracks-not-applicable_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source :database_000;
    rml:query "SELECT LTRIM(RTRIM(OperationalPoints.UOPID)) AS UOPID, Tracks.UUID, Parameters.[Index] AS PropertyIndex FROM RINF.dbo.TrackParameters, RINF.dbo.Parameters, RINF.dbo.OPTracks, RINF.dbo.Tracks, RINF.dbo.OperationalPoints WHERE OPTracks.TrackID = Tracks.ID AND OPTracks.VersionID = Tracks.VersionID AND OPTracks.TrackID = TrackParameters.TrackID AND TrackParameters.ParameterId = Parameters.ID AND TrackParameters.IsApplicable = 2 AND OPTracks.OperationalPointID = OperationalPoints.ID AND OPTracks.VersionID = OperationalPoints.VersionID;";
    rr:sqlVersion rr:SQL2008.
:database_000 a d2rq:Database;
    d2rq:jdbcDSN "${RINF_HOST}";
    d2rq:jdbcDriver "com.microsoft.sqlserver.jdbc.SQLServerDriver";
    d2rq:username "${RINF_USER}";
    d2rq:password "${RINF_PWD}".
:source_000 rml:referenceFormulation ql:CSV.
:map_op-tracks-not-applicable_000 a rr:TriplesMap;
    rdfs:label "op-tracks-not-applicable".
:s_000 a rr:SubjectMap.
:map_op-tracks-not-applicable_000 rr:subjectMap :s_000.
:s_000 rr:template "http://data.europa.eu/949/functionalInfrastructure/tracks/{UOPID}_{UUID}";
    rr:graphMap :gm_000.
:gm_000 a rr:GraphMap;
    rr:constant "${NAMED_GRAPH}".
:pom_000 a rr:PredicateObjectMap.
:map_op-tracks-not-applicable_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant era:notApplicable.
:rules_000 void:exampleResource :map_op-tracks-not-yet-available_000.
:map_op-tracks-not-yet-available_000 rml:logicalSource :source_001.
:source_001 a rml:LogicalSource;
    rml:source :database_001;
    rml:query "SELECT LTRIM(RTRIM(OperationalPoints.UOPID)) AS UOPID, Tracks.UUID, Parameters.[Index] AS PropertyIndex FROM RINF.dbo.TrackParameters, RINF.dbo.Parameters, RINF.dbo.OPTracks, RINF.dbo.Tracks, RINF.dbo.OperationalPoints WHERE OPTracks.TrackID = Tracks.ID AND OPTracks.VersionID = Tracks.VersionID AND OPTracks.TrackID = TrackParameters.TrackID AND TrackParameters.ParameterId = Parameters.ID AND TrackParameters.IsApplicable = 4 AND OPTracks.OperationalPointID = OperationalPoints.ID AND OPTracks.VersionID = OperationalPoints.VersionID;";
    rr:sqlVersion rr:SQL2008.
:database_001 a d2rq:Database;
    d2rq:jdbcDSN "${RINF_HOST}";
    d2rq:jdbcDriver "com.microsoft.sqlserver.jdbc.SQLServerDriver";
    d2rq:username "${RINF_USER}";
    d2rq:password "${RINF_PWD}".
:source_001 rml:referenceFormulation ql:CSV.
:map_op-tracks-not-yet-available_000 a rr:TriplesMap;
    rdfs:label "op-tracks-not-yet-available".
:s_001 a rr:SubjectMap.
:map_op-tracks-not-yet-available_000 rr:subjectMap :s_001.
:s_001 rr:template "http://data.europa.eu/949/functionalInfrastructure/tracks/{UOPID}_{UUID}";
    rr:graphMap :gm_001.
:gm_001 a rr:GraphMap;
    rr:constant "${NAMED_GRAPH}".
:pom_001 a rr:PredicateObjectMap.
:map_op-tracks-not-yet-available_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant era:notYetAvailable.
:rules_000 void:exampleResource :map_index-predicates_000.
:map_index-predicates_000 rml:logicalSource :source_002.
:source_002 a rml:LogicalSource;
    rml:source "data/index_predicate_mappings.csv";
    rml:referenceFormulation ql:CSV.
:map_index-predicates_000 a rr:TriplesMap;
    rdfs:label "index-predicates".
:s_002 a rr:SubjectMap.
:map_index-predicates_000 rr:subjectMap :s_002.
:s_002 rml:reference "predicate".
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:parentTriplesMap :map_index-predicates_000;
    rr:joinCondition :jc_000.
:jc_000 rr:child "PropertyIndex";
    rr:parent "index".
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rr:parentTriplesMap :map_index-predicates_000;
    rr:joinCondition :jc_001.
:jc_001 rr:child "PropertyIndex";
    rr:parent "index".
