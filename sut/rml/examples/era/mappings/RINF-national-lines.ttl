:map_line-national-ids_000 rr:subjectMap :s_000.
:s_000 rr:template "http://data.europa.eu/949/functionalInfrastructure/nationalLines/{UICCode}";
    rr:graphMap :gm_000.
:gm_000 a rr:GraphMap;
    rr:constant "${NAMED_GRAPH}".
:pom_000 a rr:PredicateObjectMap.
